{
  "version": "0.0.1",
  "configurations": [
    {
      "name": "Static Site (npx serve)",
      "runtimeExecutable": "npx",
      "runtimeArgs": ["serve", ".", "-p", "8080", "--no-clipboard"],
      "port": 8080
    },
    {
      "name": "Static Site (Python)",
      "runtimeExecutable": "python",
      "runtimeArgs": ["-m", "http.server", "8081"],
      "port": 8081
    }
  ]
}
